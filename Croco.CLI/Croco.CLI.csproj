  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Croco.Core\Croco.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\PublishProfiles\" />
  </ItemGroup>

</Project>
